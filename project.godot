config_version=5

[application]

config/name="Dungeons And Weapons"
run/main_scene="res://Scenes/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://art/sprites/icons/coin.png"

[autoload]

GameSaver="*res://scripts/Autoloads/game_saver.gd"
AiPathMarkers="*res://scripts/Autoloads/ai_path_markers.gd"
ResourceLoadingManager="*res://scripts/Autoloads/resource_loading_manager.gd"
DragAndDropLayer="*res://Scenes/Autoloads/DragAndDropLayer.tscn"
MainStateMachine="*res://state_machines/main_state_machine/main_state_machine.gd"
GameStateMachine="*res://state_machines/game_state_machine/game_state_machine.gd"
AudioMixer="*res://audio/mixer/audio_mixer.gd"
ResourceResetter="*res://scripts/Autoloads/resource_resetter.gd"
InputTypeChecker="*res://scripts/Autoloads/input_type_checker.gd"
ButtonClick="*res://Scenes/button_click.tscn"
MenuMusicPlayer="*res://Scenes/Autoloads/MenuMusicPlayer.tscn"

[display]

window/size/viewport_width=512
window/size/viewport_height=512
window/size/mode=2
window/stretch/mode="viewport"
window/stretch/scale=2.0
window/stretch/scale_mode="integer"

[file_customization]

folder_colors={
"res://player/": "red",
"res://script_templates/": "yellow",
"res://visitor/": "red"
}

[gui]

theme/custom="res://themes/default_theme.tres"
theme/custom_font="res://fonts/minimal5x7/minimal5x7.otf"

